{
  "name": "typescript-express-boilerplate",
  "version": "1.0.0",
  "description": "typescript-express-boilerplate",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "husky install",
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "nodemon src/index.ts",
    "migrate": "prisma migrate dev --skip-generate --skip-seed --name",
    "generate": "prisma generate",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --fix --ext .ts",
    "spellcheck": "cspell 'src/'",
    "test": "jest --forceExit --coverage src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saptarshi1996/typescript-express-boilerplate.git"
  },
  "keywords": [],
  "author": "Saptarshi Sengupta",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saptarshi1996/typescript-express-boilerplate/issues"
  },
  "homepage": "https://github.com/saptarshi1996/typescript-express-boilerplate#readme",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
